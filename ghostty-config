# theme
theme = dark:GitHub Dark High Contrast,light:GitHub Light Default

# keybinds
keybind = ctrl+a>l=goto_split:next
keybind = ctrl+a>h=goto_split:previous
keybind = ctrl+a>b=toggle_split_zoom

keybind = ctrl+a>u=scroll_page_up
keybind = ctrl+a>d=scroll_page_down

keybind = ctrl+a>n=clear_screen
